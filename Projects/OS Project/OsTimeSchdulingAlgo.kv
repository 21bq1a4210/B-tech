WindowManager:
    MainWindow:
    FCFS:
    SJF:
    RR:
    Priority:

<MainWindow>:
    name:"main"
    GridLayout:
        cols:2

        Label:
            text:"FCFS"
            font_size:32
        Button:
            text:"Click Here"
            on_release:
                app.root.current = "fcfs"
                root.manager.transition.direction="left"

        Label:
            text:"SJF"
            font_size:32
        Button:
            text:"Click Here"
            on_release:
                app.root.current = "sjf"
                root.manager.transition.direction="left"

        Label:
            text:"RR"
            font_size:32
        Button:
            text:"Click Here"
            on_release:
                app.root.current = "rr"
                root.manager.transition.direction="left"

        Label:
            text:"Priority"
            font_size:32
        Button:
            text:"Click Here"
            on_release:
                app.root.current = "priority"
                root.manager.transition.direction="left"

<FCFS>:
    name:"fcfs"
    BoxLayout:
        orientation:"vertical"
        Label:
            text:"FCFS Schduling Algo"
            font_size:46
            size_hint:None,.25
            pos_hint:{"right":.5}
            color:0,0,1,1
        Tabel:
            id:tabel
        GridLayout:
            cols:2
            size_hint:.5,None
            pos_hint:{"right":.5}
            Button:
                text:"Submit"
                on_press:root.clicked_fcfs(tabel.pid,tabel.at,tabel.bt,tabel.ct,tabel.tat,tabel.wt)

            Button:
                text:"Go TO Main"
                on_release:
                    app.root.current = "main"
                    root.manager.transition.direction="right"

<SJF>:
    name:"sjf"
    BoxLayout:
        orientation:"vertical"

        Tabel:

        GridLayout:
            cols:2
            size_hint:.5,None
            pos_hint:{"right":.5}
            Button:
                text:"Submit"
                #on_press:root.clicked_sjf(self)

            Button:
                text:"Go TO Main"
                on_release:
                    app.root.current = "main"
                    root.manager.transition.direction="right"

<RR>:
    name:"rr"
    BoxLayout:
        orientation:"vertical"

        Tabel:

        GridLayout:
            cols:2
            size_hint:.5,None
            pos_hint:{"right":.5}
            Button:
                text:"Submit"
                #on_press:root.clicked_rr(self)

            Button:
                text:"Go TO Main"
                on_release:
                    app.root.current = "main"
                    root.manager.transition.direction="right"

<Priority>:
    name:"priority"
    BoxLayout:
        orientation:"vertical"

        Tabel:

        GridLayout:
            cols:2
            size_hint:.5,None
            pos_hint:{"right":.5}
            Button:
                text:"Submit"
                on_press:
                    #root.submit(self)
                    root.clicked_priority(self)


            Button:
                text:"Go TO Main"
                on_release:
                    app.root.current = "main"
                    root.manager.transition.direction="right"

